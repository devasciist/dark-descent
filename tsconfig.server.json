{
	"compilerOptions": {
		"target": "esnext",
		"module": "NodeNext",
		"lib": [
			"ESNext",
		],
		"rootDir": "./server",
		"outDir": "./dist",
		"skipLibCheck": true,
		"moduleResolution": "nodenext",
		"verbatimModuleSyntax": true,
		"strict": true,
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"noFallthroughCasesInSwitch": true,
		"noUncheckedSideEffectImports": true,
		"forceConsistentCasingInFileNames": true,
	},
	"include": [
		"server"
	],
	"exclude": [
		"src"
	]
}